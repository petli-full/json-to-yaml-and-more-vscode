{
	"publisher": "petli-full",
	"name": "json-to-yaml-and-more",
	"displayName": "convert json, yaml, xml, and others",
	"description": "convert one type to another for: json, yaml, xml, csv, hjson, json5, ini, and toml",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"repository": "https://github.com/petli-full/json-to-yaml-and-more-vscode",
	"license": "MIT",
	"icon": "images/favicon.png",
	"galleryBanner": {
		"color": "#F2F2F2",
		"theme": "light"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:json-to-yaml-and-more.open-query-box"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "json-to-yaml-and-more.open-query-box",
				"title": "JSON, YAML, XML, and more"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"ts-loader": "^8.0.4",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^5.0.0",
		"webpack-cli": "^4.0.0"
	},
	"dependencies": {
		"any-json-no-cson": "^3.1.7",
		"rxjs": "^6.6.3"
	}
}